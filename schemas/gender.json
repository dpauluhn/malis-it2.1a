{
  "$id": "gender.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Gender",
  "description": "Das Schema für das Gender-Feld",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uri"
      },
      "label": {
        "type": "string",
        "enum": ["Weiblich", "Männlich"]
      }
    },
    "required": ["id", "label"]
  }
}